@*Burası categorylist default*@
@model IEnumerable<EntityLayer.Concrete.Category>
<div class="tech-btm">
	<h4>Kategoriler</h4>
	<ul class="list-group single">
@foreach(var item in Model){
		<li class="list-group-item d-flex justify-content-between align-items-center">
		@item.CategoryName
				<span class="badge badge-primary badge-pill">@if (item.CategoryStatus == true){<h5>Aktif</h5>}else{<h5>Pasif</h5>}
				</span>
		</li>
}
	</ul>
</div>